{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src/", "prisma/", "./"],
                "extensions": [".ts"]
            }
        }
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "camelcase": "off",
        "no-unused-vars": "off"
    }
}
